<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="50dp"
        android:height="10dp">
        <shape>
            <solid android:color="@android:color/holo_green_dark" />
            <!--<corners-->
            <!--android:bottomLeftRadius="2dp"-->
            <!--android:topLeftRadius="2dp"/>-->
        </shape>
    </item>

    <item
        android:width="7dp"
        android:height="7dp"
